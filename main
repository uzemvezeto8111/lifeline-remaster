local gameId = 155615604
local Teams = game:GetService("Teams")

if game.PlaceId == gameId then

-- Library
local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()

-- User Interface
local UserInterface = Rayfield:CreateWindow({
   Name = "Lifeline Hub REMASTERED | Prison Life",
   Icon = 0, -- Icon in Topbar. Can use Lucide Icons (string) or Roblox Image (number). 0 to use no icon (default).
   LoadingTitle = "Lifeline Hub REMASTERED",
   LoadingSubtitle = "by uzemvezeto",
   ShowText = "Lifeline Hub REMASTERED", -- for mobile users to unhide rayfield, change if you'd like
   Theme = "Amethyst", -- Check https://docs.sirius.menu/rayfield/configuration/themes

   ToggleUIKeybind = Enum.KeyCode.RightShift, -- The keybind to toggle the UI visibility (string like "K" or Enum.KeyCode)

   DisableRayfieldPrompts = false,
   DisableBuildWarnings = true, -- Prevents Rayfield from warning when the script has a version mismatch with the interface

   ConfigurationSaving = {
      Enabled = true,
      FolderName = nil, -- Create a custom folder for your hub/game
      FileName = "LifelifeREMASTERED_save"
   },

   Discord = {
      Enabled = false, -- Prompt the user to join your Discord server if their executor supports it
      Invite = "noinvitelink", -- The Discord invite code, do not include discord.gg/. E.g. discord.gg/ ABCD would be ABCD
      RememberJoins = true -- Set this to false to make them join the discord every time they load it up
   },

   KeySystem = false, -- Set this to true to use our key system
   KeySettings = {
      Title = "Untitled",
      Subtitle = "Key System",
      Note = "No method of obtaining the key is provided", -- Use this to tell the user how to get a key
      FileName = "Key", -- It is recommended to use something unique as other scripts using Rayfield may overwrite your key file
      SaveKey = true, -- The user's key will be saved, but if you change the key, they will be unable to use your script
      GrabKeyFromSite = false, -- If this is true, set Key below to the RAW site you would like Rayfield to get the key from
      Key = {"Hello"} -- List of keys that will be accepted by the system, can be RAW file links (pastebin, github etc) or simple strings ("hello","key22")
   }
})

-- General tab
local GeneralTab = UserInterface:CreateTab("General", "cog") -- Title, Image


-- Team swapper
local TeamSwapperSection = GeneralTab:CreateSection("Team Swapper")
local Player = game.Players.LocalPlayer
local teams = game:GetService("Teams")

local guardsTeam = teams:FindFirstChild("Guards")
local inmatesTeam = teams:FindFirstChild("Inmates")
local neutralTeam = teams:FindFirstChild("Neutral")
local criminalsTeam = teams:FindFirstChild("Criminals")

function swapGuards()
    if Player.Team == guardsTeam then
        Rayfield:Notify({
        Title = "ERROR",
        Content = "You are already in the GUARDS team",
        Duration = 3.5,
        Image = "minus",
        })

    else
        Player.Team = guardsTeam
    end 
end

function swapInmates()
    if Player.Team == inmatesTeam then
        Rayfield:Notify({
        Title = "ERROR",
        Content = "You are already in the INMATES team",
        Duration = 3.5,
        Image = "minus",
        })

    else
        Player.Team = inmatesTeam
    end 
end

function swapNeutral()
    if Player.Team == neutralTeam then
        Rayfield:Notify({
        Title = "ERROR",
        Content = "You are already in the NEUTRAL team",
        Duration = 3.5,
        Image = "minus",
        })

    else
        Player.Team = neutralTeam
    end 
end

function swapCriminals()
    if Player.Team == criminalsTeam then
        
        Rayfield:Notify({
        Title = "ERROR",
        Content = "You are already in the CRIMINALS team",
        Duration = 3.5,
        Image = "minus",
        })

    else
        Player.Team = criminalsTeam
    end 
end

local swapGuardBtn = GeneralTab:CreateButton({
   Name = "Swap to Guard",
   Callback = function()
   swapGuards()
   end,
})

local swapInmates = GeneralTab:CreateButton({
   Name = "Swap to Inmates",
   Callback = function()
   swapInmates()
   end,
})

local swapNeutral = GeneralTab:CreateButton({
   Name = "Swap to Neutral",
   Callback = function()
   swapNeutral()
   end,
})

local swapCriminals = GeneralTab:CreateButton({
   Name = "Swap to Criminals",
   Callback = function()
   swapCriminals()
   end,
})




    
    -- Teleport tab
    local TeleportTab = UserInterface:CreateTab("Teleport", "circle")
    local TeleportSection = TeleportTab:CreateSection("Teleport")
    
    local player = game.Players.LocalPlayer
    local character = player.Character or player.CharacterAdded:Wait()

    local TeleportCriminalBase = TeleportTab:CreateButton({
        Name = "Teleport Criminal Base #1",
        Callback = function()
            
            local player = game.Players.LocalPlayer
            local character = player.Character or player.CharacterAdded:Wait()
            

            local targetCFrame = CFrame.new(-975, 109, 2061)  

            character:SetPrimaryPartCFrame(targetCFrame)
        end,
     })

     local TeleportToCriminalBase2 = TeleportTab:CreateButton({
        Name = "Teleport Criminal Base #2",
        Callback = function()
            
            local player = game.Players.LocalPlayer
            local character = player.Character or player.CharacterAdded:Wait()
            

            local targetCFrame = CFrame.new(-920, 95, 2138)  

            character:SetPrimaryPartCFrame(targetCFrame)
        end,
     })

     local TeleportToPrisonGate = TeleportTab:CreateButton({
        Name = "Teleport Prison Main Gate",
        Callback = function()
            
            local player = game.Players.LocalPlayer
            local character = player.Character or player.CharacterAdded:Wait()
            

            local targetCFrame = CFrame.new(470, 98, 2216)  

            character:SetPrimaryPartCFrame(targetCFrame)
        end,
     })

     local TeleportToPrisonGarage = TeleportTab:CreateButton({
        Name = "Teleport Prison Garage",
        Callback = function()
            
            local player = game.Players.LocalPlayer
            local character = player.Character or player.CharacterAdded:Wait()
            

            local targetCFrame = CFrame.new(611, 98, 2467)  

            character:SetPrimaryPartCFrame(targetCFrame)
        end,
     })

     
     local TeleportToPrisonYard = TeleportTab:CreateButton({
        Name = "Teleport Prison Yard",
        Callback = function()
            
            local player = game.Players.LocalPlayer
            local character = player.Character or player.CharacterAdded:Wait()
            

            local targetCFrame = CFrame.new(836, 98, 2515)  

            character:SetPrimaryPartCFrame(targetCFrame)
        end,
     })

     local TeleportToPrisonEntrance = TeleportTab:CreateButton({
        Name = "Teleport Prison Main Entrance",
        Callback = function()
            
            local player = game.Players.LocalPlayer
            local character = player.Character or player.CharacterAdded:Wait()
            

            local targetCFrame = CFrame.new(651, 99, 2272)  

            character:SetPrimaryPartCFrame(targetCFrame)
        end,
     })


    -- Misc Tab
    local MiscTab = UserInterface:CreateTab("Misc", "group") -- Title, Image

    local DeleteAllDoors = MiscTab:CreateButton({
        Name = "Delete All Doors (Local, Only Prison)",
        Callback = function()
            local workspace = game:GetService("Workspace")
            local doors = workspace:FindFirstChild("Doors")

            if doors then
                for _, child in pairs(doors:GetChildren()) do
                    if child:IsA("BasePart") or child:IsA("Model") then
                        child:Destroy()
                    end
                end
            end
        end,
    })


    local DeleteAllFences = MiscTab:CreateButton({
        Name = "Delete All Fences (Local, Only Prison)",
        Callback = function()
            local workspace = game:GetService("Workspace")
            local fences = workspace:FindFirstChild("Prison_Fences")

            if fences then
                for _, part in pairs(fences:GetChildren()) do
                    if part:IsA("BasePart") or part:IsA("Model") then
                        part:Destroy()
                    end
                end
            end
        end,
    })


    local DeletePrisonWalls = MiscTab:CreateButton({
        Name = "Delete Outer Prison Wall (Local)",
        Callback = function()
            local workspace = game:GetService("Workspace")
            local outerwall = workspace:FindFirstChild("Prison_OuterWall")

            if outerwall then
                outerwall:Destroy()
            end
        end,
    })

    local SelfTab = UserInterface:CreateTab("Self", "rabbit")

    local function UpdateCharacter()
        local character = game.Players.LocalPlayer.Character
        local humanoid = character and character:FindFirstChildOfClass("Humanoid")
    
        return character, humanoid
    end
    

    local WalkSpeedSlider = SelfTab:CreateSlider({
        Name = "Walk Speed",
        Range = {0, 999},
        Increment = 10,
        Suffix = "Speed",
        CurrentValue = 16,
        Flag = "WalkSpeedChanger", 
        Callback = function(Value)
            local character, humanoid = UpdateCharacter()
            if humanoid then
                humanoid.WalkSpeed = Value
            end
        end,
    })
    

    local JumpPowerSlider = SelfTab:CreateSlider({
        Name = "Jump Power",
        Range = {0, 999},
        Increment = 10,
        Suffix = "Power",
        CurrentValue = 50,
        Flag = "JumpPowerChanger", 
        Callback = function(Value)
            local character, humanoid = UpdateCharacter()
            if humanoid then
                humanoid.JumpPower = Value
            end
        end,
    })
    

    game.Players.LocalPlayer.CharacterAdded:Connect(function()

        local character, humanoid = UpdateCharacter()

        if humanoid then
            humanoid.WalkSpeed = WalkSpeedSlider.CurrentValue
            humanoid.JumpPower = JumpPowerSlider.CurrentValue
        end
    end)


    

    local ResetAll = SelfTab:CreateButton({
        Name = "Reset Values",
        Callback = function()

            local character, humanoid = UpdateCharacter()
            if humanoid then
                humanoid.WalkSpeed = 16
                humanoid.JumpPower = 50  
            end
    
            WalkSpeedSlider:Set(16)
            JumpPowerSlider:Set(50)
        end,
    })

    

























--Close the script
local CloseScript = GeneralTab:CreateSection("Close")
local close = GeneralTab:CreateButton({
   Name = "Close",
   Callback = function()
   Rayfield:Destroy()
   end,
})



else
    print("This game is not supported.")

end

